<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="View.App"
             RequestedThemeVariant="Default">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="Assets/Converters.axaml"/>
				<ResourceInclude Source="Assets/Images.axaml"/>
				<ResourceInclude Source="Assets/DataTemplates.axaml"/>
				<ResourceInclude Source="Assets/Resources.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
	<Application.Styles>
		<FluentTheme />
		<StyleInclude Source="avares://CalendarControl/Themes/Default.axaml" />
		<StyleInclude Source="avares://Avalonia.Controls.DataGrid/Themes/Fluent.xaml"/>
	</Application.Styles>
</Application>
